{
    "name": "honey-comb/payments",
    "description": "",
    "keywords": ["honeycomb", "honey", "laravel", "payments"],
    "homepage": "https://github.com/honey-comb/payments/",
    "license": "MIT",
    "authors": [
        {
            "name": "Jevgenij Dmitrijev",
            "email": "jd@interactivesolutions.lt"
        },
        {
            "name": "Mantas Paulauskas",
            "email": "mp@interactivesolutions.lt"
        },
        {
            "name": "Zygimantas Gylys",
            "email": "zygimantas.gylys@gmail.com"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*",
        "honey-comb/starter": "*",
        "honey-comb/core": "*",
        "honey-comb/resources": "*"
    },
    "require-dev": {
        "brianium/paratest": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench-browser-kit": "^3.5",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "\\HoneyComb\\Payments\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HoneyComb\\Payments\\Providers\\HCPaymentsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "paratest -p4 --colors"
        ]
    },
    "config": {
        "secure-http": false,
        "platform": {
            "php": "7.1"
        }
    }
}